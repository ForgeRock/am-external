<!DOCTYPE propertysheet SYSTEM "propertysheet.dtd">

<propertysheet>
    <section name="config" defaultValue="create.soap.sts.deployment.section.config">
        <property required="true">
            <label name="lblRealm"
                   defaultValue="create.soap.sts.deployment.label.realm"
                   labelFor="tfRealm" />
            <cc name="tfRealm" tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
            </cc>
            <fieldhelp name="helpRealm" defaultValue="create.soap.sts.deployment.realm.help"/>
        </property>
        <property required="true">
            <label name="lblOpenAMUrl" defaultValue="create.soap.sts.deployment.label.openam.url"
                   labelFor="tfOpenAMUrl" />
            <cc name="tfOpenAMUrl" tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag">
            </cc>
            <fieldhelp name="helpOpenAMUrl" defaultValue="create.soap.sts.deployment.openam.url.help"/>
        </property>
        <property required="true">
            <label name="lblSoapAgentName" defaultValue="create.soap.sts.deployment.label.soap.agent.name"
                   labelFor="tfSoapAgentName" />
            <cc name="tfSoapAgentName" tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag">
            </cc>
            <fieldhelp name="helpSoapAgentName" defaultValue="create.soap.sts.deployment.soap.agent.name.help"/>
        </property>
        <property required="true">
            <label name="lblSoapAgentPassword" defaultValue="create.soap.sts.deployment.label.soap.agent.password"
                   labelFor="tfSoapAgentPassword" />
            <cc name="tfSoapAgentPassword" tagclass="com.sun.web.ui.taglib.html.CCPasswordTag">
            </cc>
            <fieldhelp name="helpSoapAgentPassword" defaultValue="create.soap.sts.deployment.soap.agent.password.help"/>
        </property>
        <property required="true">
            <label name="lblSoapAgentPasswordConfirm" defaultValue="create.soap.sts.deployment.label.soap.agent.password.confirm"
                   labelFor="tfSoapAgentPassword_confirm" />
            <cc name="tfSoapAgentPassword_confirm" tagclass="com.sun.web.ui.taglib.html.CCPasswordTag">
            </cc>
        </property>
        <property required="true">
            <label name="lblSoapAgentRetryNumber" defaultValue="create.soap.sts.deployment.label.soap.agent.retry.number"
                   labelFor="tfSoapAgentRetryNumber" />
            <cc name="tfSoapAgentRetryNumber" tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag">
            </cc>
            <fieldhelp name="helpSoapAgentRetryNumber" defaultValue="create.soap.sts.deployment.soap.agent.retry.number.help"/>
        </property>
        <property required="true">
            <label name="lblSoapAgentRetryInitialInterval" defaultValue="create.soap.sts.deployment.label.soap.agent.retry.initial.interval"
                   labelFor="tfSoapAgentRetryInitialInterval" />
            <cc name="tfSoapAgentRetryInitialInterval" tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag">
            </cc>
            <fieldhelp name="helpSoapAgentRetryInitialInterval" defaultValue="create.soap.sts.deployment.soap.agent.retry.initial.interval.help"/>
        </property>
        <property required="true">
            <label name="lblSoapAgentRetryMultiplier" defaultValue="create.soap.sts.deployment.label.soap.agent.retry.multiplier"
                   labelFor="tfSoapAgentRetryMultiplier" />
            <cc name="tfSoapAgentRetryMultiplier" tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag">
            </cc>
            <fieldhelp name="helpSoapAgentRetryMultiplier" defaultValue="create.soap.sts.deployment.soap.agent.retry.multiplier.help"/>
        </property>
        <property required="false">
            <label name="lblCustomWsdlFileNames" defaultValue="create.soap.sts.deployment.label.custom.wsdl.file.names"
                   labelFor="elCustomWsdlFileNames" />
            <cc name="elCustomWsdlFileNames"
                tagclass="com.sun.web.ui.taglib.editablelist.CCEditableListTag">
                <attribute name="name" value="list" />
                <attribute name="listboxLabel" value="label.current.value" />
                <attribute name="textboxLabel" value="label.new.value" />
            </cc>
            <fieldhelp name="helpCustomWsdlFileNames" defaultValue="create.soap.sts.deployment.custom.wsdl.file.names.help"/>
        </property>
        <property required="false">
            <label name="lblKeystoreFileNames" defaultValue="create.soap.sts.deployment.label.keystore.file.names"
                   labelFor="elKeystoreFileNames" />
            <cc name="elKeystoreFileNames"
                tagclass="com.sun.web.ui.taglib.editablelist.CCEditableListTag">
                <attribute name="name" value="list" />
                <attribute name="listboxLabel" value="label.current.value" />
                <attribute name="textboxLabel" value="label.new.value" />
            </cc>
            <fieldhelp name="helpKeystoreFileNames" defaultValue="create.soap.sts.deployment.keystore.file.names.help"/>
        </property>
    </section>
</propertysheet>
