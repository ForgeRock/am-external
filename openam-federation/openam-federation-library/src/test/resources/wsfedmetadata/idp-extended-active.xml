<FederationConfig FederationID="openam-wsfed-idp" hosted="true" xmlns="urn:sun:fm:wsfederation:1.0:federationconfig">
    <IDPSSOConfig metaAlias="/idp">
        <Attribute name="displayName">
            <Value>/idp</Value>
        </Attribute>
        <Attribute name="nameIdFormat">
            <Value/>
        </Attribute>
        <Attribute name="nameIdAttribute">
            <Value/>
        </Attribute>
        <Attribute name="nameIncludesDomain">
            <Value/>
        </Attribute>
        <Attribute name="domainAttribute">
            <Value/>
        </Attribute>
        <Attribute name="upnDomain">
            <Value>example.com</Value>
        </Attribute>
        <Attribute name="signingCertAlias">
            <Value>defaultkey</Value>
        </Attribute>
        <Attribute name="assertionNotBeforeTimeSkew">
            <Value>600</Value>
        </Attribute>
        <Attribute name="assertionEffectiveTime">
            <Value>600</Value>
        </Attribute>
        <Attribute name="idpAuthncontextMapper">
            <Value>com.sun.identity.wsfederation.plugins.DefaultIDPAuthenticationMethodMapper</Value>
        </Attribute>
        <Attribute name="idpAccountMapper">
            <Value>com.sun.identity.wsfederation.plugins.TestIdpDefaultAccountMapper</Value>
        </Attribute>
        <Attribute name="idpAttributeMapper">
            <Value>com.sun.identity.wsfederation.plugins.DefaultIDPAttributeMapper</Value>
        </Attribute>
        <Attribute name="activeRequestorProfileEnabled">
            <Value>true</Value>
        </Attribute>
        <Attribute name="authenticatorClass">
            <Value>com.sun.identity.wsfederation.plugins.TestWsFedAuthenticator</Value>
        </Attribute>
        <Attribute name="trustedAddresses">
            <Value>urn:federation:MicrosoftOnline</Value>
        </Attribute>
        <Attribute name="endpointBaseUrl">
            <Value>http://openam.example.com:8080/am</Value>
        </Attribute>
        <Attribute name="attributeMap">
            <Value/>
        </Attribute>
        <Attribute name="cotlist">
            <Value>TestCOT</Value>
        </Attribute>
    </IDPSSOConfig>
</FederationConfig>