<Federation FederationID="openam-wsfed-idp" xmlns="http://schemas.xmlsoap.org/ws/2006/12/federation">
    <TokenSigningKeyInfo>
        <ns1:SecurityTokenReference ns1:Usage="" xmlns:ns1="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
            <ns2:X509Data xmlns:ns2="http://www.w3.org/2000/09/xmldsig#">
                <ns2:X509Certificate></ns2:X509Certificate>
            </ns2:X509Data>
        </ns1:SecurityTokenReference>
    </TokenSigningKeyInfo>
    <TokenIssuerName>openam-wsfed-idp</TokenIssuerName>
    <TokenIssuerEndpoint>
        <ns3:Address xmlns:ns3="http://www.w3.org/2005/08/addressing">http://openam.example.com:8080/am/WSFederationServlet/metaAlias/idp</ns3:Address>
    </TokenIssuerEndpoint>
    <TokenTypesOffered>
        <TokenType Uri="urn:oasis:names:tc:SAML:1.1"/>
    </TokenTypesOffered>
    <UriNamedClaimTypesOffered>
        <ClaimType Uri="http://schemas.xmlsoap.org/claims/UPN">
            <DisplayName>UPN</DisplayName>
        </ClaimType>
    </UriNamedClaimTypesOffered>
</Federation>
