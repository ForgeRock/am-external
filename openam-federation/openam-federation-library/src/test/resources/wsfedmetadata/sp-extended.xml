<FederationConfig FederationID="external-wsfed-sp" hosted="false" xmlns="urn:sun:fm:wsfederation:1.0:federationconfig">
    <SPSSOConfig metaAlias="/sp">
        <Attribute name="displayName">
            <Value>/sp</Value>
        </Attribute>
        <Attribute name="AccountRealmSelection">
            <Value>cookie</Value>
        </Attribute>
        <Attribute name="AccountRealmCookieName">
            <Value>amWSFederationAccountRealm</Value>
        </Attribute>
        <Attribute name="HomeRealmDiscoveryService">
            <Value>http://external.example.com:7070/am/RealmSelection/metaAlias/sp</Value>
