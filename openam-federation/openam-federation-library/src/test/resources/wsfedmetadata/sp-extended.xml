        <Attribute name="assertionCacheEnabled">
            <Value>true</Value>
        </Attribute>
        <Attribute name="wantAssertionSigned">
            <Value>true</Value>
        </Attribute>
        <Attribute name="cotlist">
            <Value>TestCOT</Value>
        </Attribute>
    </SPSSOConfig>
</FederationConfig>